var game = new mini2d.Game(600, 480, '.container');
game.load(['assets/paddle.png']);
game.init = function(){
    game.debug.add(function () {
        return 'cursor position x: ' + game.input.x + ' y: ' + game.input.y;
    });
    game.debug.add(function () {
        return 'mouse down: ' + game.input.mouseDown;
    });
    game.debug.add(function () {
        return 'camera position x: ' + game.camera.position.x + ' y: ' + game.camera.position.y;
    });
};
