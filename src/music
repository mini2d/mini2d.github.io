var game = new mini2d.Game(600, 480, '.container');
game.load(['assets/btn.png', 'assets/btn-active.png', 'assets/music.mp3']);
game.init = function(){
    var sound = game.audio.addSound('assets/music.mp3');
    game.ui.addButton(0, 0, function () {
        console.log("audio must be initialized with touchend event on iOS");
        sound.play();
    }, 'assets/btn.png', 'assets/btn-active.png').addText('init iosSound', '15px sans-serif', '#fff');
};
game.update = function(){
}
